<Window x:Class="Batch_Rename.MoveConfigDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Batch_Rename"
        mc:Ignorable="d"
        Title="MoveConfigDialog" Height="200" Width="300">
    <StackPanel Margin="10">
        <Label FontWeight="Bold">Please select a model:</Label>
        <RadioButton Name="ISBN_FileName_RadioButton" IsChecked="True">ISBN - FileName</RadioButton>
        <RadioButton Name="FileName_ISBN_RadioButton">FileName - ISBN</RadioButton>
        <Button x:Name="okButton" Click="OkButton_Click" Content="Ok" Width="80" Height="25" HorizontalAlignment="Center" Margin="10"/>
    </StackPanel>
</Window>
